@article{cohenRoleAcceleratorDesigns2019,
  title = {The {{Role}} of {{Accelerator Designs}} in {{Mitigating Bounded Rationality}} in {{New Ventures}}},
  author = {Cohen, Susan L. and Bingham, Christopher B. and Hallen, Benjamin L.},
  date = {2019-12-01},
  journaltitle = {Administrative Science Quarterly},
  volume = {64},
  number = {4},
