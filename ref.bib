  keywords = {Comprehensive Model,Economic Growth,Industrial Organization,Initial Model,Technology Management},
  file = {/var/home/bevan/Zotero/storage/CGQQNXBV/Peters et al. - 2004 - The Role of Incubators in the Entrepreneurial Proc.pdf}
