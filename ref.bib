@article{leitaoBusinessIncubatorsAccelerators2022,
  title = {Business {{Incubators}}, {{Accelerators}}, and {{Performance}} of {{Technology-Based Ventures}}: {{A Systematic Literature Review}}},
  shorttitle = {Business {{Incubators}}, {{Accelerators}}, and {{Performance}} of {{Technology-Based Ventures}}},
  author = {Leitão, João and Pereira, Dina and Gonçalves, Ângela},
